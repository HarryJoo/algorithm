import java.lang.Math;

class Solution {
  public long solution(long n) {
      long answer = 0;
      Double sqrtValD = Math.sqrt(n);
      int sqrtVal = (int)Math.sqrt(n);
      if(sqrtValD == sqrtVal){
          answer = (long)Math.pow(sqrtValD+1, 2);
      }else{
          answer = -1;
      }
          
      return answer;
  }
}
